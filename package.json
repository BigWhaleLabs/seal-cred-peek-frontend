{
  "name": "seal-cred-peek-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts",
    "prestart": "yarn generate-css-types",
    "prebuild": "yarn generate-css-types"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.1.19",
    "@big-whale-labs/seal-cred-ledger-contract": "^0.2.6",
    "@big-whale-labs/stores": "^0.1.10",
    "@vitejs/plugin-react": "^2.0.0",
    "@zk-kit/incremental-merkle-tree": "^0.4.3",
    "chart.js": "^3.9.1",
    "envalid": "^7.3.1",
    "ethers": "^5.6.9",
    "preact": "^10.10.1",
    "react-chartjs-2": "^4.3.1",
    "react-router-dom": "^6.3.0",
    "secure-ls": "^1.2.6",
    "valtio": "^1.6.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.3.0",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.5.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "tailwindcss": "^3.1.8",
    "tailwindcss-classnames": "^3.0.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "packageManager": "yarn@3.1.0"
}
